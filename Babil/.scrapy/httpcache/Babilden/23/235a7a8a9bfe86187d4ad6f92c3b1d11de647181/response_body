<script language="javascript" type="text/javascript">
		function ajaxVariation(variationId) {
			//LoadProductImageBox(variationId);
			LoadProductExtraBox(variationId);
			LoadProductInfo(variationId);
		}

		function LoadBreadCrumb(categoryId) {
			$.ajax({
				type: 'get',
				dataType: 'html',
				scripts: true,
				data: 'categoryId=' + categoryId,
				url: '/Catalog/Products/BreadCrumb/',
				success: function (data) { $('#Catalog_Products_BreadCrumb').html(data); },
				error: function (data) {}
			});
		}

		function LoadProductImageBox(variationId) {
			DefaultScript.ShowIndicator('Yükleniyor', 'Catalog_Products_ProductImageBox');
			console.log(variationId);
			$.ajax({
				type: 'get',
				dataType: 'html',
				scripts: true,
				data: 'id=' + variationId,
				url: '/Catalog/Products/ProductImageBoxNew/',
				success: function (data) { $('#Catalog_Products_ProductImageBox').html(data); DefaultScript.HideIndicator('Catalog_Products_ProductImageBox'); },
				error: function (data) { alert(data.responseText); }
			});
		}

		function LoadProductExtraBox(variationId) {
			$.ajax({
				type: 'get',
				dataType: 'html',
				scripts: true,
				url: '/Catalog/Products/ProductExtraBoxNew/' + variationId,
				success: function (data) { $('#Catalog_Products_ProductExtraBox').html(data); },
				error: function (data) { alert(data.responseText); }
			});
		}

		function LoadProductInfo(variationId) {
			$.ajax({
				type: 'post',
				dataType: 'html',
				scripts: true,
				url: '/Catalog/Products/ProductInfoNew/' + variationId,
				success: function (data) { $('#Catalog_Products_ProductInfo').html(data); },
				error: function (data) { alert(data.responseText); }
			});
		}

		function LoadProductComment(variationId) {
			$.ajax({
				type: 'get',
				dataType: 'html',
				scripts: true,
				url: '/Catalog/Products/ProductComment/' + variationId,
				success: function (data) { $('#Catalog_Products_ProductComment').html(data); },
				error: function (data) { alert(data.responseText); }
			});
		}
	</script>

