    var SiteTranslate = (function () {
        var _translate = {};
        _translate.translate_WARNING = 'Uyarı';
        _translate.translate_ERROR = 'Hata';
        _translate.translate_QUANTITYMORETHANSTOCK = 'Adet Stoktan Fazladır';
