<script type="text/javascript" language="javascript">
    //TopMenu Search
    var _productTrans = 'urunler';

    $("#TopProductName").bind('keydown', function Gonder(e) {
        if (e.keyCode == 13) Search();
    });

    $("#TopProductName").autocomplete("/Main/Public/ProductSearchExtended", {
        dataType: 'json',
        extraParams: { categoryId: function() { return $("#ddlCategoryId").val(); } },
        minLength: 2,
        selectFirst: false,
        parse: function(data) {
            var rows = new Array();
            for (var i = 0; i < data.length; i++) {
                rows[i] = { data: data[i], result: data[i].SearchText };
            }
            return rows;
        },
        formatItem: function(row, i, n) {
            return '<span class="searchProductName">' + row.SearchText + '</span><span class="search-item-field"> (' + row.SearchTextFieldDisplayName + ') </span> <!-- <img class="searchImg" src="' + row.Image + '" /> <span class="searchPrice">' + row.Price + ' ' + row.CurrencyCode + ' </span> -->';
        }
    }).result(function(event, data, formatted) {
        var catId = $("#ddlCategoryId").val();
		var searchText = data.SearchText;
		//searchText = searchText.replace(/\)|\(/ig,'');
		//searchText = searchText.replace(/\-/ig,' ');
		searchText = searchText +' ' + data.SearchTextFieldType;
		searchText = searchText.replace(" " + data.SearchTextFieldType, "");
		debugger;
        var link = '/Catalog/Products/Search?productName=' + '"' + encodeURIComponent(searchText) + '"' + '&categoryId=' + catId + "&displayName=" + data.SearchTextFieldDisplayName;
        document.location.href = link;

    });

    function Search() {
        var hid = $("#TopProductName").val();
		//hid = hid.replace(/\)|\(/ig,'');
		//hid = hid.replace(/\-/ig,' ');
        var catId = $("#ddlCategoryId").val();
        if (hid.indexOf("ÃrÃ¼n") >= 0 || hid.indexOf("Ã¼rÃ¼n") >= 0) {
            hid = "";
        }
        window.location = '/Catalog/Products/Search?productName=' + encodeURIComponent(hid) + '&categoryId=' + catId + '&decode=false';
    };

    $(document).ready(function() {
    $('#ddlCategoryId').val('0');
        $('#ddlCategoryId').dropkick({
            theme: 'search',
            setValue: '0'
        });
    });

</script>
