<script type="text/javascript">
    $(document).ready(function() {
        var height = $('.active div:eq(0)').height();
        height = height + 70;
        $('.product-tab').css('height', height);
        $('.product-tab li').click(function() {
            $('.product-tab li').removeClass("active");
            $(this).addClass("active");

            var height = $('.active div:eq(0)').height();
            height = height + 70;
            $('.product-tab').css('height', height);
        });
    });
    function AddShoppingList() {
        var variationId = '1236735';
        $.ajax({
            type: 'post',
            dataType: 'json',
            data: { 'variationId': variationId },
            url: '/Catalog/Products/AddShoppingList',
            success: function(data) {
                if (data.Status) {
                    // $.colorbox({ inline: true, href: '#dlgShoppingList' });
                    DefaultScript.PNotify('Alışveriş Listenize Eklendi', 'Alışveriş Listenize Eklendi', 'success');
                } else {
                    DefaultScript.PNotify('', data.Message, 'error');
                }
            },
            error: function() { }
        });

    };
    function OpenWishForm() {
        var productId = '551400';
        $.ajax({
            type: 'post',
            data: { 'productId': productId },
            url: '/Catalog/Products/WishForm',
            success: function(data) {
                $('#divWishForm').html(data);
                $.colorbox({ href: '#divWishForm', inline: 'true' });
            }
        });
    }
    function AddToWishForm() {

        var productId = '551400';
        $.ajax({
            type: 'post',
            dataType: 'json',
            data: { 'productId': productId },
            url: '/Catalog/Products/AddWishForm',
            success: function(data) {

                DefaultScript.PNotify('', data.Message, 'success');

            },
            error: function() { }
        });
    }
</script>

<script type="text/javascript">

    
    $(window).load(function() {
        productType = $("#hiddenproductType").val();       

        if (productType=="Ebook" || productType=="PDF")
        {            
            $("#spanCalibrotxt").html('<img src="http://images1.babil.com/Content/Uploads/logo/okuyabileceginiz-cihazlar.png" alt="ekitap okuyabileceğiniz platformlar"> <a title="ekitap okuyabileceğiniz platformlar calibro" href="http://www.babil.com/Main/Home/PageContent/bilgi-calibro" target="_blank"><img src="http://images1.babil.com/Content/Uploads/logo/calibro.png" alt="ekitap okuyabileceğiniz platformlar"> </a><a title="ekitap okuyabileceğiniz platformlar iOS" href="https://itunes.apple.com/us/app/calibro-reader/id958205905?l=tr" target="_blank"><img src="http://images1.babil.com/Content/Uploads/logo/ios.png" alt="ekitap okuyabileceğiniz platformlar"> </a><a title="ekitap okuyabileceğiniz platformlar android" href="https://play.google.com/store/apps/details?id=com.calibro.reader&hl=tr" target="_blank"><img src="http://images1.babil.com/Content/Uploads/logo/android.png" alt="ekitap okuyabileceğiniz platformlar"> </a> <br /> <br />');
            barcode=$("#hdnBarcode").val();
            if (barcode != undefined && barcode.length>0)
            {
                $("#spnbarcode").html($("#hdnBarcode").val());
            }
            
        }
		
        $("img.book-small").fadeIn(500);

        // clone image
        $('img.book-small').each(function() {
            var el = $(this);
            el.css({ "position": "absolute" }).wrap("<div class='img_wrapper' style='display: inline-block'>").clone().addClass('img_grayscale').css({ "position": "absolute", "z-index": "998", "opacity": "0" }).insertBefore(el).queue(function() {
                var el = $(this);
                el.parent().css({ "width": this.width, "height": this.height });
                el.dequeue();
            });
            this.src = grayscale(this.src);
        });

        // Fade image
        //        $('img.book-small').mouseover(function() {
        //            $(this).parent().find('img:first').stop().animate({ opacity: 1 }, 1000);
        //        })
        //        $('.img_grayscale').mouseout(function() {
        //            $(this).stop().animate({ opacity: 0 }, 1000);
        //        });
    });

    // Grayscale w canvas method
    function grayscale(src) {
        var canvas = document.createElement('canvas');
        var ctx = canvas.getContext('2d');
        var imgObj = new Image();
        imgObj.src = src;
        canvas.width = imgObj.width;
        canvas.height = imgObj.height;
        ctx.drawImage(imgObj, 0, 0);
        var imgPixels = ctx.getImageData(0, 0, canvas.width, canvas.height);
        for (var y = 0; y < imgPixels.height; y++) {
            for (var x = 0; x < imgPixels.width; x++) {
                var i = (y * 4) * imgPixels.width + x * 4;
                var avg = (imgPixels.data[i] + imgPixels.data[i + 1] + imgPixels.data[i + 2]) / 3;
                imgPixels.data[i] = avg;
                imgPixels.data[i + 1] = avg;
                imgPixels.data[i + 2] = avg;
            }
        }
        ctx.putImageData(imgPixels, 0, 0, 0, 0, imgPixels.width, imgPixels.height);
        return canvas.toDataURL();
    }
    function OpenInstallmetntOptions() {
        var productId = '551400';
        var amount = '17,5';
        $.ajax({
            type: 'post',
            data: { 'amount': amount, 'productId': productId },
            url: '/Banks/Home/GetCreditCardOptions',
            success: function(data) {
                $('#divTaksit').html(data);
                $.colorbox({ href: '#divTaksit', inline: 'true' });
            }
        });
    }
