<script type="text/javascript">
	$(function () {
		$("#txtQuantity").spinit({ height: 30, width: 50, min:1, initValue: 1, max: 10});
		CheckQuantity();
	});

	function CheckQuantity() {
		var quantity = $("#txtQuantity").val();
		var stockAmount = $("#hiddstockAmout").val();
		var maxOrder = $("#hiddmaxOrder").val();
		var minOrder = $("#hiddminOrder").val();
	}

	function SetCustomizationParameter() {
		var parameter = '';
		if($('#frmCustomizationProduct').length > 0) {
			var selectedCustom = $('#frmCustomizationProduct select:[id^="custom_"]');
			$.each(selectedCustom, function (index, item) {
				if ($(item).val() == '1') {
					var ids = $(item).attr('id');
					var id = ids.split('_')[1];
					var value = $('#customValue_' + id).val();
					value = value.replace(/;/g, "");
					value = value.replace(/~/g, "");
					if (value != '')
						parameter += id + '~' + value + ';'
				} else {
				}
			});
		}
		return parameter;
	}

	var AddToBasketWidget = (function () {
		var basket = {};
		basket.quantity = 10;
		basket.variationId = 1236735;
		basket.stockAmount = 77;
		basket.minOrderQuantity = 1;
		basket.maxOrderQuantity = 10;
		basket.criticalStockLevel=0;
		basket.SetQuantity = function (element) {
			if(basket.CheckQuantity(element))
				basket.quantity = $(element).val();
		};

		basket.CheckQuantity=function(element){
			var tempQuantity = $(element).val();
			if(tempQuantity == "" || parseInt(tempQuantity) <= 0){DefaultScript.PNotify('Uyarı', 'Hata', ''); return false;}
			if(parseInt(basket.maxOrderQuantity) != 0 && parseInt(tempQuantity) > parseInt(basket.maxOrderQuantity)){DefaultScript.PNotify('Uyarı', 'Adet Maksimum Sipariş Adetinden daha fazla olamaz.', '');return false;}
			if(parseInt(basket.minOrderQuantity) != 0 && parseInt(tempQuantity) < parseInt(basket.minOrderQuantity)){DefaultScript.PNotify('Uyarı', 'Miktar min. sipariş miktarından az', '');return false;}
			if(parseInt(tempQuantity) > parseInt(basket.stockAmount)){DefaultScript.PNotify('Uyarı', 'Adet Stoktan Fazladır', '');return false;}
			if(parseInt(basket.stockAmount) < parseInt(basket.criticalStockLevel)){DefaultScript.PNotify('Uyarı', 'Adet Stoktan Fazladır', '');return false;}
			return true;
		};

		basket.addToBasket = function () {
			var isLogin = $("#isLogin").val();
			//var variation1=$("#hdnVariationOptionId1").val();
			var variation2=$("#hdnVariationOptionId2").val();
			var selectedDate=$("#hdnSelectedDate").val();
			//var variation3=$("#hdnVariationOptionId3").val();
			var checkedCount = 0;
			var chkVarExists = false;
			$("[name^=chkVariationId_]").each(function(){
				if($(this).is(':checked')) {
					checkedCount = checkedCount+1;
				}
				chkVarExists = true;
			});
			if(chkVarExists && 1 > checkedCount) {
				DefaultScript.PNotify('Uyarı', 'Varyasyon Seçiniz.', '');
				return;
			}
			//if(!basket.CheckQuantity('#txtQuantity')) return;
			basket.quantity = $("#txtQuantity").val();
			var customizationParameters = SetCustomizationParameter();
			$.ajax({
				type:'post',
				data:{'variationId':basket.variationId,'quantity':basket.quantity,'custamizationParameters' : customizationParameters,'selectedDate':selectedDate},
				url:'/Catalog/Baskets/AddToBasket',
				success:function(data){
					$('#divBasketPopUp').html(data);
					DefaultScript.ReloadWidget('BasketQuantity',  '/Catalog/Baskets/BasketQuantity','');
					DefaultScript.ReloadWidget('MiniBasket',  '/Catalog/Baskets/MiniBasket','');
                     DefaultScript.ReloadWidget('divBasketCenter', '/Catalog/Baskets/BasketCenter', ''); 
					if(isLogin=="True" || isLogin =="true" ){
						$("div#MiniBasket").show();
					} else {
						$.colorbox({href:'#divBasketPopUp' , inline:'true'});
					}
				}
			});
		};

		basket.addToBasketJson = function(){
			var variation2=$("#hdnVariationOptionId2").val();
			var selectedDate=$("#hdnSelectedDate").val();
			var checkedCount = 0;
			var chkVarExists = false;
			$("[name^=chkVariationId_]").each(function(){
				if($(this).is(':checked')) {
					checkedCount = checkedCount+1;
				}
				chkVarExists = true;
			});
			if(chkVarExists && 1 > checkedCount) {

				DefaultScript.PNotify('Uyarı', 'Varyasyon Seçiniz.', '');
				return;
			}

			if(selectedDate=="-1" || selectedDate=="" ) {
				DefaultScript.PNotify('Uyarı', 'Varyasyon Seçiniz.', '');
				return;
			}
			if(!basket.CheckQuantity('#txtQuantity')) return;
			var customizationParameters = SetCustomizationParameter();
			basket.quantity = $("#txtQuantity").val();
			$.ajax({
				type:'post',
				dataType:'json',
				data:{'variationId':basket.variationId,'quantity':basket.quantity,'selectedDate':selectedDate},
				url:'/Catalog/Baskets/AddToBasketJson',
				success:function(data){
					if(data.isOk){
						window.location='/Catalog/Baskets/Index';
					} else{
					DefaultScript.PNotify('Uyarı', data.message, '');
					}
				}
			});
		};
		return {
			SetQuantity : basket.SetQuantity,
			AddToBasket : basket.addToBasket,
			AddToBasketJson : basket.addToBasketJson
		};

	} ());
</script>
