<script type="text/javascript">
    function CreateBxSlider(divId, itemCount)
    {
        var value = false;
        var currentItemCount = $('#' + divId + ' li').length;
        if(currentItemCount > itemCount)
        {
            currentItemCount = itemCount;
            value = true;
        }
        $('#' + divId).bxSlider({ floatMode: 'none', auto: true, 
            displaySlideQty: currentItemCount, 
            startSlide: 0, 
            moveSlideQty: currentItemCount, 
            pager: false, 
            infiniteLoop:value, controls:value, pause:60000 });
    }
</script>
