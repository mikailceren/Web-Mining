<script type="text/javascript" language="javascript">

    var pageMiniLogin = (function () {
        var process = {};

        process.email = '';
        process.password = '';
        process.rememberMe = '0';

        process.Translate = {
            UnavilableEmailFormat: 'Geçersiz E-posta formatı',
		    EmailEmpty: 'Lütfen E-Postanızı Giriniz',
		    PasswordEmpty: 'Lütfen Şifrenizi Giriniz',
		    Warning: 'Uyarı',
		    Info: 'Bilgi'
		};

	    process.SetRememberMe = function (element) {
	        if ($(element).is(':checked')) {
	            process.rememberMe = 1;
	        }
	        else {
	            process.rememberMe = 0;
	        }
	    };

	    process.SetEmail = function (element) {
	        if (!DefaultScript.CheckEmail($(element).attr('id'))) {
	            DefaultScript.PNotify(process.Translate.Info, process.Translate.UnavilableEmailFormat, 'error');
	            process.email = '';
	            $(element).val('');
	            return false;
	        }
	        process.email = $(element).val();
	        return true;
	    };

	    process.SetPassword = function (element) {
	        process.password = $(element).val();
	        if (process.password == '') return false;
	        return true;
	    };

	    process.SendLoginParameter = function () {
	        process.SetEmail('#txtLoginEmail');
	        process.SetPassword('#txtPasswordEmail');
	        if (DefaultScript.IsEmpty(process.email)) {
	            DefaultScript.PNotify(process.Translate.Info, process.Translate.EmailEmpty, 'error');
	            return;
	        }

	        if (DefaultScript.IsEmpty(process.password)) {
	            DefaultScript.PNotify(process.Translate.Info, process.Translate.PasswordEmpty, 'error');
	            return;
	        }
	        $.ajax({
	            type: 'post',
	            dataType: 'json',
	            data: { 'email': process.email, 'password': process.password, 'rememberMe': process.rememberMe },
	            url: '/Main/Home/EntryControl',
	            success: function (data) {
	                if (data.isSuccess) {
	                    window.location = '/Main/Home/Index';
	                }
	                else {
	                    DefaultScript.PNotify(process.Translate.Warning, data.message, 'error');
	                }
	            }
	        });
	    };

	    return {
	        SetEmail: process.SetEmail,
	        SetPassword: process.SetPassword,
	        SendLoginParameter: process.SendLoginParameter,
	        SetRememberMe: process.SetRememberMe,
	    }

	}());

</script>


<script type="text/javascript">

    var siteHeaderWidget = (function () {
        var process = {};
        process.Exit = function () {
            $.ajax({
                type: 'post',
                dataType: 'json',
                url: '/Main/Home/CustomerExit',
                success: function (data) {
                    if (data == 1) {
                        DefaultScript.PNotify('Bilgi', 'Sistemden başarılı bir şekilde çıkış yaptınız ', '');
					    window.location = '/Main/Home/Index';
					}
				}
			});
		};
	    return {
	        Exit: process.Exit
	    };
	}());

</script>
