        <script type="text/javascript">
            $(document).ready(function() {
                $('.product-tab-style li a').click(function() {
                    $('html, body').animate({
                        scrollTop: $($(this).attr('href')).offset().top
                    }, 500);
                    return false;
                });
                $('.fragman a').click(function() {
                    //debugger;
                    $('html, body').animate({
                        scrollTop: $($(this).attr('href')).offset().top
                    }, 500);
                    return false;
                });
            });
        </script>

        
