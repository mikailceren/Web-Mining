<script type="text/javascript">
    $(document).ready(function() {
        $('#spine_image').one("load", function() {
            if (('turkce-kitap' != 'film' && 'PaperBoard' != 'DVD') || 'PaperBoard' == 'VCD' ) {
                var vWidth = $(this).width() / -2;
                var vHeight = $("#big_image").height();

                $('.bk-left').css({ 'left': vWidth });
                $('#spine_image').css({ 'height': vHeight });
                $('#spine_image').css({ 'width': '100%' });

                var transformVal = 'translate3d(0,0,' + ($(this).width() / 2) + 'px)';
                $('.bk-front').css({ 'transform': transformVal });
                $('.bk-front').css({ '-webkit-transform': transformVal });
                $('.bk-front').css({ '-moz-transform': transformVal });

                //var transformValBack = 'rotate3d(0,1,0,-180deg) translate3d(0,0,' + ($(this).width() / 2) + 'px)';

                $('.bk-list .bk-back').css({ 'transform': 'rotate3d(0,1,0,-180deg) translate3d(0,0,' + ($(this).width() / 2) + 'px)' });
                $('.bk-list .bk-back').css({ '-webkit-transform': 'rotate3d(0,1,0,-180deg) translate3d(0,0,' + ($(this).width() / 2) + 'px)' });
                $('.bk-list .bk-back').css({ '-moz-transform': 'rotate3d(0,1,0,-180deg) translate3d(0,0,' + ($(this).width() / 2) + 'px)' });
            }

        }).each(function () {
            if (this.complete) $(this).load(); // cachelenen resimde de tetiklenmesi için
        });

         $('.big_image.image_for_spin').one("load", function() {
            var vHeight = $(this).height();

            $('.turkce-kitap .bk-spin-book').css({ 'height': vHeight });

        }).each(function () {
            if (this.complete) $(this).load(); // cachelenen resimde de tetiklenmesi için
        });

        var Books = (function() {

            var $books = $('#bk-list  > li > div.bk-book');

            function init() {
                $books.each(function() {

                    var $book = $(this),
                    $parent = $book.parent();

                    $parent.find('div.bk-bookback').on('click', function() {
                        if ($book.data('flip')) {
                            $book.data({ opened: false, flip: false }).removeClass('bk-viewback').addClass('bk-bookdefault');
                        }
                        else {
                            $book.data({ opened: false, flip: true }).removeClass('bk-bookdefault').addClass('bk-viewback');
                        }
                    });

                    $parent.find('.image_for_spin').on('click', function() {
                        if ($book.data('flip')) {
                            $book.data({ opened: false, flip: false }).removeClass('bk-viewback').addClass('bk-bookdefault');
                        }
                        else {
                            $book.data({ opened: false, flip: true }).removeClass('bk-bookdefault').addClass('bk-viewback');
                        }
                    });

                    $parent.find('.bk-spin-book').on('click', function() {
                        if ($book.data('flip')) {
                            $book.data({ opened: false, flip: false }).removeClass('bk-viewback').addClass('bk-bookdefault');
                        }
                        else {
                            $book.data({ opened: false, flip: true }).removeClass('bk-bookdefault').addClass('bk-viewback');
                        }
                    });
                });
            }
            return { init: init };
        })();

        Books.init();
    });
</script>

