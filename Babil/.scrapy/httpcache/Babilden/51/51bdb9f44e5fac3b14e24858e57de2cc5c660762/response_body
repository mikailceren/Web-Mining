<script type="text/javascript">
    
    $(document).ready(function() {
        productType = $("#hiddenproductType").val();       

        if (productType!="Ebook" && productType!="PDF")
        {
            var productId= 551400            
            var price=17.5
            $.ajax({
                type: 'get',
                dataType: 'json',
                data: { 'productId' :productId,'price' : price },
                url: '/Catalog/Products/ShipmentFreeText',
                success: function (data) {
                    $("#spanCargoFree").text(data);  
                },
                error: function () { }
            });


           // $("#spanCargoFree").text(' ');   
        }

    });


    $(function () {
        //ProductRating.Init();
        $("#divProductRating").raty({
            half:false,
            score:'3',
            click: function(score) {
                var productId= 551400;
                $.ajax({
                    type: 'post',
                    dataType: 'json',
                    data: { 'productId' : productId ,'value':score },
                    url: '/Catalog/Products/ProductRatingSave',
                    success: function (data) {
                        DefaultScript.PNotify(data.Title, data.Message, data.MessageType);
                        
                        $('#divProductRating').raty('score',data.Data);
                        $('#ratingTotalAmount').val(data.Data1);
                        //$('#stars').rating('readOnly',true);
                        //$('#hover-test').html('');
                    },
                    error: function () { }
                });
            }

        });
       
    });


    

        var ProductRating = (function () {
            var rating = {};
            rating.dlgWindow = null;

            rating.Init = function () {
                rating.InitEventHandler();
            };


            rating.InitEventHandler = function () {
          
    
                $('#stars').rating({
                    //focus: function(value, link){
                    //    var tip = $('#hover-test');
                    //    tip[0].data = tip[0].data || tip.html();
                    //    tip.html(link.title || 'value: '+value);
                    //},
                    //    blur: function(value, link){
                    //    var tip = $('#hover-test');
                    //    $('#hover-test').html(tip[0].data || '');
                    //},
                    callback:function(value, title){rating.SaveProductRating(value);}
                });
                var value = 3;
            $('#ratingTotalAmount').val(9);
            value = parseInt(value-1);
            $('#stars').rating('select',value, false);
            $('#stars').rating('readOnly',true);
            $('#hover-test').html('');
                
        };

        rating.SaveProductRating = function (value) { 
                
            var productId= 551400;
            $.ajax({
                type: 'post',
                dataType: 'json',
                data: { 'productId' : productId ,'value':value },
                url: '/Surveys/Home/ProductRatingSave',
                success: function (data) {
                    DefaultScript.PNotify(data.Title, data.Message, data.MessageType);
                    $('#stars [value="'+data.Data+'"]').attr('checked',true);
                    $('#stars').rating('readOnly',true);
                    $('#hover-test').html('');
                    $('#ratingTotalAmount').val(data.Data1);
                },
                error: function () { }
            });
        };

        return {
            Init: rating.Init,
            SaveProductRating: rating.SaveProductRating
        };

    } ());
</script>

                               
