    var siteHeaderWidget = (function () {
        var process = {};
        process.ChangeLanguage = function (languageId) {
            $.ajax({
                type: 'post',
                dataType: 'json',
                data: { 'languageId': languageId },
                url: '/Main/Home/ChangeLanguageOfUser',
                success: function (data) {
                    if (data.Status) {
                        window.location.reload();
                    }
                }
            });
        };
        process.Exit = function () {
            $.ajax({
                type: 'post',
                dataType: 'json',
                url: '/Main/Home/CustomerExit',
                success: function (data) {
                    if (data == 1) {
                        DefaultScript.PNotify('Bilgi', 'Sistemden başarılı bir şekilde çıkış yaptınız ', '');
                        window.location = '/';
                    }
                }
            });
        };
        return {
            Exit: process.Exit,
            ChangeLanguage: process.ChangeLanguage
        };
    }());

    //TopMenu Search
    //var _productTrans = '';
    var r = { 'special': /['<''>''+''~''*''\'']/g, 'quotes': /['\''&'\"']/g, 'notnumbers': /[^\d]/g }

    function valid(o, w) {
        o.value = o.value.replace(r[w], '');
    }
    function ChangeCurrency(languageId) {
        var currencyId = "";
        if (languageId == 1) {
            currencyId = 1;
        }
        else {
            currencyId = 2;
        }

        $.ajax({
            type: 'post',
            dataType: 'json',
            data: { 'currencyId': currencyId },
            url: '/Main/Home/ChangeCurrencyOfUser',
            success: function (data) {

                if (data.Status) {
                    window.location.reload();
                }
            }
        });
    }
</script>

