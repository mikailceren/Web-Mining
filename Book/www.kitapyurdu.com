<script type="text/javascript"><!--
$('#password').keydown(function(e) {
	if (e.keyCode == 13) {
	    ajaxLogin();
	}
    });

    function ajaxLogin() {

	var email = $("#email").val();
	var password = $("#password").val();
	var rememberme = $("#rememberme").is(':checked');
	$.ajax({
	    xhrFields: {
		withCredentials: true
	    },
	    type: "POST",
	    url: "https://www.kitapyurdu.com/index.php?route=account/login/dialogLogin",
	    data: "email=" + encodeURIComponent(email) + "&password=" + encodeURIComponent(password) + "&rememberme=" + rememberme,
	    dataType: 'json',
	    beforeSend: function(xhr) {
		$('#login-dialog').append('<span class="wait"><i class="fa fa-spinner fa-3x fa-spin"></i></span>');
	    },
	    complete: function(jqXHR, textStatus) {
		$('#login-dialog .wait').remove();
	    },
	    success: function(json) {
		if (json.success === true) {

		    window.location.reload();
		} else {
		    $('#login-dialog .dialog-warning').html(json.message);
		    $('#login-dialog .dialog-warning').show();
		}
	    }
	});
    }


//--></script></div>


    
