    function postProductToFeed(product_id, type, description) {

	$.ajax({
	    url: 'http://www.kitapyurdu.com/index.php?route=product/product/facebookPostInformation',
	    type: 'get',
	    data: 'product_id=' + product_id,
	    success: function(response) {
		var product = jQuery.parseJSON(response);
		var title = "";
		if (type == "review") {
		    title = product.name + "hakkında '" + description + "' yorumunu yaptı";
		}
		postToFeed(title, product.description, product.name, product.share_url, product.image);
	    }
	});
    }



    function postToFeed(name, description, hrefTitle, hrefLink, image) {

	FB.ui(
		{
		    method: 'stream.publish',
		    name: name,
		    link: hrefLink,
		    picture: image,
		    caption: hrefTitle,
		    description: description
		},
	function(response) {
	    if (response && response.post_id) {
		alert('Yorumu paylaştığınız için teşekkür ederiz.');
	    } else {
		alert('Upps paylaşmadın.');
	    }
	}
	);

    }
    function postReviewToWall(product_id, review_message) {
	postProductToFeed(product_id, "review", review_message);
    }
</script>

